 public static List<Integer> rotateLeft(int d, List<Integer> arr) {
   
    int r[]=new int[arr.size()];
    ArrayList<Integer> a=new ArrayList<>();
    int j=0;
    for(int i=d;i<arr.size();i++){
        r[j]=arr.get(i);
        j++;
    }
    int s=0;
    for(int i=j;i<arr.size();i++){
        r[i]=arr.get(s);
        s++;
    }
    for(int i=0;i<arr.size();i++){
        a.add(r[i]);
    }
    return a;
    

    }

}
